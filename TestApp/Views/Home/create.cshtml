@model TestApp.Controllers.SomethingViewModel

@{
	ViewBag.Title = "Create Something";
}

<h2>Create</h2>

<form method="post" action='@Url.Action("Create")'>
	<div>
		<div>@Html.LabelFor(m => m.FullName)</div>
		<div>@Html.TextBoxFor(m => m.FullName)</div>
	</div>
	<div>
		<div>@Html.LabelFor(m => m.FullAddress)</div>
		<div>@Html.TextBoxFor(m => m.FullAddress)</div>
	</div>
	<div>
		<input type="submit" id="createButton" value="Create" />
	</div>
</form>
